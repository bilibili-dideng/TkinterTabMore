<?xml version="1.0" encoding="UTF-8"?>
<idea-plugin>
  <id>com.dideng.MPTT.MorePyCharmTkinterTab</id>
  <name>Tkinter Tab More</name>
  <version>1.0</version>
  <vendor url="https://github.com/bilibili-dideng ">dideng</vendor>

  <resource-bundle>messages.Bundle</resource-bundle>

  <description><![CDATA[
        Enhances Tkinter development in PyCharm by providing smart code completion for common protocol strings.
    ]]></description>

  <change-notes><![CDATA[
        <ul>
            <li>Initial release with support for Tkinter protocol string completion.</li>
        </ul>
    ]]></change-notes>

  <idea-version since-build="231" until-build="251.*"/>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.intellij.modules.python</depends>
  <extensions defaultExtensionNs="com.intellij">
    <completion.contributor
            language="Python"
            implementationClass="com.dideng.mptt.morepycharmtkintertab.TkinterCompletionContributor"/>
  </extensions>
</idea-plugin>